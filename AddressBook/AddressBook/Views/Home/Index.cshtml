@model IEnumerable<AddressBook.Models.Contacts>

@{
    ViewBag.Title = @Resources.Views.MainPage;
}

<div class="userBarInfo">@TempData["ChangeDBInfo"]</div>

<h2>@Resources.Views.ContactsList</h2>

<p>
    @Html.ActionLink(@Resources.Views.Create, "Create", null, new { @class = "linkButton" })
</p>

<div id="contactList">
    @Html.Partial("ContactList", Model)
</div>

@section Scripts {
    @Scripts.Render("~/bundles/ajax")
}